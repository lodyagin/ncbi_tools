\f2\fs18\lang1033\langfe1033\cgrid\langnp1033\langfenp1033 {\insrsid2252114   #include <accentr.h>
\par 
\par   ValNodePtr  elst;
\par   Int2        group;
\par   Int4        i;
\par   Int2        last;
\par   LinkSetPtr  lsp;
\par   DocUid      uid;
\par 
\par   group = 0;
\par   last = 0;
\par 
\par   while (MORE_QUERIES) \{
\par     elst = EntrezTLNew (TYPE);
\par     while (GET_NEXT_TERM ()) \{
\par 
\par       if (GROUP_SINGLE || GROUP_FIRST) \{
\par         group++;
\par }\pard \s27\ql \li0\ri0\widctlpar\brdrt\brdrs\brdrw15\brsp20 \brdrl\brdrs\brdrw15\brsp20 \brdrb\brdrs\brdrw15\brsp20 \brdrr\brdrs\brdrw15\brsp20 \tx360\tx720\tx1080\tx1440\faauto\adjustright\rin0\lin0\itap0 {\insrsid2252114       \}
\par 
\par       if (last == 0) \{
\par         EntrezTLAddLParen (elst);
\par       \} else if (last == group) \{
\par         EntrezTLAddOR (elst);
\par       \} else \{
\par         EntrezTLAddRParen (elst);
\par         EntrezTLAddAND (elst);
\par         EntrezTLAddLParen (elst);
\par       \}
\par 
\par       EntrezTLAddTerm (elst, TERM, TYPE, FIELD, STATE_SPECIAL);
\par       last = group;
\par     \}
\par 
\par     lsp = EntrezTLEval (elst);
\par     EntrezTLFree (elst);
\par     for (i = 0; i < lsp->num; i++) \{
\par       uid = lsp->uids [i];
\par       /* Do something with the uid. */
\par     \}
\par     LinkSetFree (lsp);
\par   \}
\par 
\par 
\par }\pard\plain \s2\ql \li0\ri0\sb240\keep\keepn\widctlpar\faauto\outlinelevel1\adjustright\rin0\lin0\itap0 \b\f22\fs28\lang1033\langfe1033\cgrid\langnp1033\langfenp1033 {\insrsid2252114 {\*\bkmkstart _Toc294431043}{\*\bkmkstart _Toc300649443}
