\par extern Int2 AsnIoMemRead PROTO((Pointer, CharPtr, Uint2));
\par extern Int2 AsnIoMemWrite PROTO((Pointer, CharPtr, Uint2));
\par 
\par    /*** read and write to a ByteStore in memory ***/
