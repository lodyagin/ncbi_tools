$ mycc 'myargs' jzcoll
$ mycc 'myargs' jzmisc
$ mycc 'myargs' maputil
$ mycc 'myargs' mconsist
$ libr/obj/create libncbiobj.olb -
   objacces, objalign, objall, objbibli, objfeat, objgen, objloc, -
   objmedli, objpub, objres, objseq, objblock, objcode, objsset, -
   objsub, objprt, objfdef, objmime, objmdrs, objpubme, objproj, seqport, sequtil, -
   prtutil, simple, tofasta, tofile, tomedlin, valid, squntils, subutil, -
   edutil, asn2ff1, asn2ff2, asn2ff3, asn2ff4, asn2ff5, asn2ff6, ftusrstr, -
   gbfeat, gbftglob, gbparint, utilpars, ffprint, wprint, -
   satutil, seqmgr, objmgr, gather, accmmdbs, acccn3ds, -
   codon, txalign, jzcoll, jzmisc, maputil, mconsist
$ delete *.obj.*
$ rename libncbiobj.olb [-.lib]
$ !
$ ! Make libncbicdr.olb
$ !
$ mycc 'myargs' cdentrez
$ mycc 'myargs' cdromlib
$ mycc 'myargs' cdnewlib
$ mycc 'myargs' casn
$ mycc 'myargs' cdconfig
$ mycc 'myargs' objentr
