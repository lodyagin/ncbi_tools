$ mycc 'myargs' objprt
$ mycc 'myargs' objfdef
$ mycc 'myargs' objmime
$ mycc 'myargs' objmdrs
$ mycc 'myargs' objpubme
$ mycc 'myargs' objproj
$ mycc 'myargs' seqport
$ mycc 'myargs' sequtil
$ mycc 'myargs' prtutil
$ mycc 'myargs' simple
$ mycc 'myargs' tofasta
$ mycc 'myargs' tofile
$ mycc 'myargs' tomedlin
$ mycc 'myargs' valid
