$ !
$ ! Make libncbiacc.olb
$ !
$ mycc 'myargs' accentr
$ mycc 'myargs' accutils
$ libr/obj/create libncbiacc.olb accentr, accutils
$ delete *.obj.*
$ rename libncbiacc.olb [-.lib]
$ !
$ ! Make libvibrant.olb
$ !
$ define/nolog X11 decw$include     ! WAG   <X11/Xlib.h>
$ define/nolog Xm  decw$include     ! WAG   <Xm/*.h>
$ define/nolog sys sys$library      ! WAG   <sys/*.h>
$ !
$ mycc 'mywinargs' vibbutns
$ mycc 'mywinargs' vibextra
$ mycc 'mywinargs' vibfiles
