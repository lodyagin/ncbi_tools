   mmdbapi, mmdbapi1, mmdbapi2, mmdbapi3, mmdbapi4, mmdbentr
$ delete *.obj.*
$ rename libncbimmdb.olb [-.lib]
$ !
$ ! Make libncbitool.olb
$ !
$ mycc 'myargs' csim
$ mycc 'myargs' db_slist
$ mycc 'myargs' dust
$ mycc 'myargs' falign
$ mycc 'myargs' g_any
$ mycc 'myargs' sim2
