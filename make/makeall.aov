$ CC [-.corelib]ncbiargs
$ CC /define=(HAVE_STRING_H,REGEX_MALLOC) [-.corelib]regex
$ CC [-.asnlib]asnbufo
$ CC [-.asnlib]asndebin
$ CC [-.asnlib]asnenbin
